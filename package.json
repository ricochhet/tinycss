{
    "name": "@ricochhet/odin",
    "version": "0.2.0",
    "description": "A modern CSS framework",
    "main": "src/index.js",
    "scripts": {
        "prettier": "npx prettier --write .",
        "prettier:check": "npx prettier --check .",
        "test": "node test/index.js",
        "hooks": "npm install --save-dev husky lint-staged && npx husky install && npx husky add .husky/pre-commit \"npx lint-staged\"",
        "husky:install": "husky install",
        "prepare": "husky install"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "keywords": [],
    "author": "ricochhet",
    "license": "MIT",
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ricochhet/odin.git"
    },
    "bugs": {
        "url": "https://github.com/ricochhet/odin/issues"
    },
    "homepage": "https://github.com/ricochhet/odin",
    "devDependencies": {
        "husky": "^7.0.2",
        "lint-staged": "^11.1.2",
        "prettier": "^2.3.2"
    }
}
